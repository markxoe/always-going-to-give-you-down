{
  "name": "always-going-to-give-you-down",
  "version": "1.0.0",
  "description": "Always going to give you down",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "electron .",
    "build-win": "electron-builder -w -p never",
    "publish-win": "electron-builder -w -p onTagOrDraft"
  },
  "author": "Mark Oude Elberink",
  "devDependencies": {
    "electron": "^12.0.7",
    "electron-builder": "22.9.1"
  },
  "dependencies": {
    "discord-rpc": "^3.2.0"
  },
  "build": {
    "productName": "Always going to give you down",
    "copyright": "Copyright Â© 2021 Mark Oude Elberink",
    "publish": [
      {
        "provider": "github",
        "owner": "markxoe",
        "repo": "always-going-to-give-you-down"
      }
    ],
    "nsis": {
      "oneClick": "false",
      "installerLanguages": [
        "de_DE"
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    }
  }
}
